<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fbutton="http://schemas.android.com/apk/res-auto"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="kunalganglani.com.suveyji.Admin"
    android:background="@drawable/formbackground">

    <info.hoang8f.widget.FButton
        fbutton:buttonColor="#5868C2"
        fbutton:shadowColor="#25358F"
        android:minHeight="50dp"
        fbutton:cornerRadius="5dp"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="8dp"
        android:id="@+id/bUsermanage"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="User Management"
        android:layout_marginTop="100dp"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:background="@drawable/mybutton"
        />

    <info.hoang8f.widget.FButton
        fbutton:buttonColor="#5868C2"
        fbutton:shadowColor="#25358F"
        android:minHeight="50dp"
        fbutton:cornerRadius="5dp"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="8dp"
        android:id="@+id/bViewdata"
        android:layout_below="@id/bUsermanage"
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="View Data Collected"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:background="@drawable/mybutton"
        />

    <info.hoang8f.widget.FButton
        fbutton:buttonColor="#5868C2"
        fbutton:shadowColor="#25358F"
        android:minHeight="50dp"
        fbutton:cornerRadius="5dp"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="8dp"
        android:id="@+id/bAddform"
        android:layout_below="@id/bViewdata"
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Add form"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:background="@drawable/mybutton"
        />

    <info.hoang8f.widget.FButton
        fbutton:buttonColor="#5868C2"
        fbutton:shadowColor="#25358F"
        android:minHeight="50dp"
        fbutton:cornerRadius="5dp"
        fbutton:shadowEnabled="true"
        fbutton:shadowHeight="8dp"
        android:id="@+id/bAnalytics"
        android:layout_below="@id/bAddform"
        android:layout_marginTop="40dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Analytics"
        android:textColor="#ffffff"
        android:textSize="18sp"
        android:background="@drawable/mybutton"
        />
</RelativeLayout>
